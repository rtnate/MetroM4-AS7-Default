{
    "CompilerOptions": {
        "-D": [
            "__SAMD51J19A__",
            "ADAFRUIT_METRO_M4_EXPRESS",
            "__SAMD51__",
            "{build.usb_flags}",
            "USB_VID=0x239A",
            "USB_PID=0x8020",
            "USBCON",
            "USB_MANUFACTURER=\"Adafruit LLC\"",
            "USB_PRODUCT=\"Adafruit Metro M4\"",
            "__FPU_PRESENT",
            "ARM_MATH_CM4",
            "__SKETCH_NAME__=\"\"\"M4Alive.ino\"\"\"",
            "F_CPU=120000000L",
            "ARDUINO=10807",
            "ARDUINO_METRO_M4",
            "ARDUINO_ARCH_SAMD",
            "ENABLE_CACHE",
            "MAX_SPI=24000000",
            "VARIANT_QSPI_BAUD_DEFAULT=50000000"
        ],
        "-m": [
            "cpu=cortex-m4",
            "thumb"
            "float-abi=hard",
            "fpu=fpv4-sp-d16"
        ],
        "-c": true,
        "-g": true,
        "-O": "s",
        "-w": true,
        "-std": "gnu++11",
        "-f": [
            "function-sections",
            "data-sections",
            "no-threadsafe-statics",
            "no-exceptions",
            "no-rtti"
        ],
        "-nostdlib": true,
        "--param": [
            " max-inline-insns-single=500"
        ],
        "-x": true,
        "c++": true,
        "-E": true,
        "-CC": true,
        "-o":  "nul"
    },

    "LinkerOptions": {
        "LinkerScript": "linker_scripts/gcc/flash_with_bootloader.ld",
        "-L": [
            "{build.variant.path}"
        ],
        "-l": [
            "arm_cortexM4lf_math"
        ],
        "-m": [
            "float-abi=hard",
            "fpu=fpv4-sp-d16",
            "cpu=cortex-m4",
            "thumb"
        ],
        "-Wl": true,
        "--gc-sections": true,
        "-save_temps": true,
        "-T": "{linkerscript.path}",
        "--specs": [
            "=nano.specs",
            "=nosys.specs",
        ],
        "--cref": true
    }
}